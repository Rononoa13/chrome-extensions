{
    "name": "Close Tabs",
    "description": "The extension close tabs of unwanted URL's",
    "version": "0.1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "options_ui": {
        "page": "./options.html"
    },
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ]    
  }
        